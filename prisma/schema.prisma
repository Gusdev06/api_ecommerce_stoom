generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String    @unique @default(uuid())
  name      String
  email     String
  password  String
  createdAt DateTime?
  updatedAt DateTime?

  @@map("users")
}
